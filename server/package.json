{
  "name": "my-test",
  "version": "1.0.0",
  "description": "a test",
  "keywords": [],
  "author": "Raynos <raynos2@gmail.com>",
  "repository": "git+ssh://gitolite@code.uber.internal:rt/my-test.git",
  "main": "server.js",
  "homepage": "https://code.uberinternal.com/diffusion/RTMYT/",
  "bugs": {
    "url": "https://code.uberinternal.com/maniphest/task/create/",
    "email": "raynos2@gmail.com"
  },
  "dependencies": {
    "sirvice": "git+ssh://git@github.com:uber/sirvice",
    "level": "^0.18.0",
    "playdoh-server": "git+ssh://gitolite@code.uber.internal:github/uber/playdoh-server#v2.3.1",
    "process": "^0.7.0",
    "rt-on-client": "git+ssh://gitolite@code.uber.internal:rt/on-client#v1.3.0",
    "run-with": "git+ssh://gitolite@code.uber.internal:rt/run-with.git#v1.0.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "graphite-dash-gen": "git+ssh://gitolite@code.uber.internal:dispatch/graphite-dash-gen#v1.2.0",
    "istanbul": "^0.2.7",
    "jshint": "^2.5.0",
    "nodemon": "^1.0.17",
    "opn": "^0.1.2",
    "playdoh": "git+ssh://gitolite@code.uber.internal:github/uber/playdoh#v2.5.0",
    "request": "^2.34.0",
    "tap-spec": "^0.1.8",
    "tape": "git://github.com/Raynos/tape#global-hack"
  },
  "licenses": [],
  "scripts": {
    "test": "npm run jshint -s && tape ../endpoints/*/test/index.js ../business/test/index.js test/index.js | tap-spec",
    "watch": "NODE_HEAPDUMP_OPTIONS=nosignal nodemon --ignore browser/ --exec 'node' server.js",
    "fast-test": "tape ../endpoints/*/test/fast.js ../business/test/fast.js test/fast.js | tap-spec",
    "jshint": "jshint --verbose .",
    "jshint-pre-commit": "jshint --verbose $(git diff --cached --name-only --diff-filter=ACMRTUXB | grep '\\.js$')",
    "cover": "istanbul cover --root .. node_modules/.bin/tape -- ../endpoints/*/test/index.js",
    "fast-cover": "istanbul cover --root .. node_modules/.bin/tape -- ../endpoints/*/test/fast.js",
    "view-cover": "istanbul report html && opn ./coverage/index.html",
    "shrinkwrap": "playdoh shrinkwrap",
    "changelog-patch": "playdoh changelog --patch",
    "changelog-minor": "playdoh changelog --minor",
    "changelog-major": "playdoh changelog --major",
    "jenkins-install": "npm --registry=http://archive.uber.com/npm install",
    "jenkins-jshint": "jshint --verbose --reporter jslint $(git ls-files | grep '\\.js$' | sed -e \"s~^~${WORKSPACE:-.}/~\") > jshint.xml",
    "jenkins-test": "npm run jshint -s && istanbul cover --report cobertura --print none test/index.js | tee test/test.js.tap"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.3"
  },
  "private": true,
  "playdoh-version": "module@2.5.0"
}
