{
  "name": "device",
  "version": "1.0.0",
  "description": "This shows an example of writing a set of thin business logic     on top of a database",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run jshint && npm run cover",
    "jshint": "jshint --verbose .",
    "cover": "istanbul cover --print detail test/index.js && istanbul check-coverage --branches=100",
    "view-cover": "istanbul report html && opn ./coverage/index.html",
    "fast-test": "node test/fast.js | tap-spec"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cached-tape": "^1.1.0",
    "hammock": "^0.1.9",
    "istanbul": "^0.3.0",
    "jshint": "^2.5.3",
    "opn": "^1.0.0",
    "tap-spec": "^0.2.0"
  },
  "dependencies": {
    "error": "^4.1.1",
    "sirvice": "git+ssh://gitolite@code.uber.internal:github/uber/sirvice#dev"
  }
}
