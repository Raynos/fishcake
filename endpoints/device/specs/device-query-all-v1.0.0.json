{
    "description": "/devices version 1.0.0",
    "id": "http://demo.uber.com/schema/device/device-query-all-v1.0.0",
    "request": {
        "type": "object",
        "properties": {
            "userId": {
                "type": "number"
            }
        }
    },
    "response": {
        "type": "array",
        "items": {
            "$ref": "http://demo.uber.com/spec/device-v1.0.0"
        }
    },
    "error": {
        "type": "object",
        "properties": {
            "statusCode": {
                "type": "number",
                "enum": [400]
            },
            "message": {
                "type": "string"
            },
            "type": {
                "type": "string",
                "enum": [
                    "auth.common.incorrectPassword"
                ]
            }
        },
        "required": ["statusCode", "message"]
    }
}
